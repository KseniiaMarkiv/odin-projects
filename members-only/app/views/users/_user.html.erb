<div id="<%= dom_id user %>" class="card mb-3">
  <div class="card-body">
    <p class="card-text">
      <strong>Username:</strong>
      <%= user.username %>
    </p>

    <p class="card-text">
      <strong>Email:</strong>
      <%= user.email %>
    </p>

    <p class="card-text">
      <strong>Password:</strong>
      <%= user.password %>
    </p>
  </div>
</div>
